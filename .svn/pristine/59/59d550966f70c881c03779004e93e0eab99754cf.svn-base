package com.sina.book.data;

import java.util.ArrayList;

public class RecommendFamousItem {
    private String uid;
    private String headUrl;
    private String screenName;
    private String intro;
    
    private ArrayList<Book> recommendBooks = new ArrayList<Book>();

    public String getUid() {
        return uid;
    }

    public void setUid(String uid) {
        this.uid = uid;
    }

    public String getHeadUrl() {
        return headUrl;
    }

    public void setHeadUrl(String headUrl) {
        this.headUrl = headUrl;
    }

    public String getScreenName() {
        return screenName;
    }

    public void setScreenName(String screenName) {
        this.screenName = screenName;
    }

    public String getIntro() {
        return intro;
    }

    public void setIntro(String intro) {
        this.intro = intro;
    }

    public ArrayList<Book> getBooks() {
        return recommendBooks;
    }

    public void setBooks(ArrayList<Book> recommendBooks) {
        this.recommendBooks = recommendBooks;
    }
    
    public void addBook(Book book) {
        this.recommendBooks.add(book);
    }
}
