<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/PublicBg"
    android:layout_width="fill_parent"
    android:layout_height="37.33dp"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/partition_quick_rank"
        android:layout_width="wrap_content"
        android:layout_height="35.33dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@drawable/recommend_tab_0_bg"
        android:gravity="center"
        android:text="@string/rank"
        android:textColor="#C6C9D2"
        android:textSize="13.3sp" />

    <TextView
        android:id="@+id/partition_quick_new"
        android:layout_width="wrap_content"
        android:layout_height="35.33dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@drawable/recommend_tab_1_bg"
        android:gravity="center"
        android:text="@string/latest"
        android:textColor="#C6C9D2"
        android:textSize="13.3sp" />

    <TextView
        android:id="@+id/partition_quick_free"
        android:layout_width="wrap_content"
        android:layout_height="35.33dp"
        android:layout_gravity="center"
        android:layout_weight="1"
        android:background="@drawable/recommend_tab_2_bg"
        android:gravity="center"
        android:text="@string/free"
        android:textColor="#C6C9D2"
        android:textSize="13.3sp" />

</LinearLayout>