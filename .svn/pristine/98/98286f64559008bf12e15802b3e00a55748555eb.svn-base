package com.sina.book.data;

import java.util.List;

/**
 * 专题详情返回结果
 *
 * @author MarkMjw
 * @date 13-12-12.
 */
public class TopicResult {
    private TopicItem topic;

    private int total;

    private List<TopicItem> topics;

    public TopicItem getTopic() {
        return topic;
    }

    public void setTopic(TopicItem topic) {
        this.topic = topic;
    }

    public int getTotal() {
        return total;
    }

    public void setTotal(int total) {
        this.total = total;
    }

    public List<TopicItem> getTopics() {
        return topics;
    }

    public void setTopics(List<TopicItem> topics) {
        this.topics = topics;
    }
}
