package com.sina.book.control.download;

/**
 * 在线书籍下载接口
 *
 * @author MarkMjw
 */
public interface IOnlineBookListener {

    /**
     * 成功
     *
     * @param chapterId 章节Id
     * @param state     状态
     * @param type      类型
     */
    public void onSuccess(int chapterId, int state, int type);

    /**
     * 错误
     *
     * @param chapterId 章节Id
     * @param state     状态
     */
    public void onError(int chapterId, int state);
}
