package com.sina.book.data;

import java.util.ArrayList;

public class RecommendAuthorResult {
    private String name;
    private int count;
    private ArrayList<AuthorPageResult> datas = new ArrayList<AuthorPageResult>();

    public int getCount() {
        return count;
    }

    public void setCount(int count) {
        this.count = count;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public ArrayList<AuthorPageResult> getDatas() {
        return datas;
    }

    public void setDatas(ArrayList<AuthorPageResult> datas) {
        this.datas = datas;
    }

    public void addData(AuthorPageResult item) {
        this.datas.add(item);
    }

}
