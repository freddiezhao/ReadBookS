package com.sina.book.data;

import java.util.ArrayList;

public class SellFastItem {
    private String mItemType;
    private String mBookType;
    private ArrayList<Book> mBooks = new ArrayList<Book>();
    private int mBookCount;

    // 用于行为统计
    private String mItemTag = "";
    private String mBookTag = "";

    public String getItemTag() {
        return mItemTag;
    }

    public void setItemTag(String itemTag) {
        this.mItemTag = itemTag;
    }

    public String getBookTag() {
        return mBookTag;
    }

    public void setBookTag(String bookTag) {
        this.mBookTag = bookTag;
    }

    public String getItemType() {
        return mItemType;
    }

    public void setItemType(String itemType) {
        this.mItemType = itemType;
    }

    public String getBookType() {
        return mBookType;
    }

    public void setBookType(String bookType) {
        this.mBookType = bookType;
    }

    public ArrayList<Book> getBooks() {
        return mBooks;
    }

    public void setBooks(ArrayList<Book> books) {
        this.mBooks = books;
    }

    public int getBookCount() {
        return mBookCount;
    }

    public void setBookCount(int bookCount) {
        this.mBookCount = bookCount;
    }

}