package com.sina.book.data;

import java.util.ArrayList;

/**
 * 搜索页面数据信息
 * 
 * @author YangZhanDong
 * 
 */
public class SearchData {

	private Book mBook;
	private ArrayList<String> mHotWords;
	private ArrayList<Book> mHotBooks;
	private String mBookType;
	private String mBookTypeName;
	// recommand
	private String mRecommandName;

	public String getRecommandName() {
		return mRecommandName;
	}

	public void setRecommandName(String mRecommandName) {
		this.mRecommandName = mRecommandName;
	}

	public Book getBook() {
		return mBook;
	}

	public void setBook(Book book) {
		this.mBook = book;
	}

	public ArrayList<String> getHotWords() {
		return mHotWords;
	}

	public void setHotWords(ArrayList<String> hotWords) {
		this.mHotWords = hotWords;
	}

	public ArrayList<Book> getHotBooks() {
		return mHotBooks;
	}

	public void setHotBooks(ArrayList<Book> hotBooks) {
		this.mHotBooks = hotBooks;
	}

	public String getBookType() {
		return mBookType;
	}

	public void setBookType(String bookType) {
		this.mBookType = bookType;
	}

	public String getBookTypeName() {
		return mBookTypeName;
	}

	public void setBookTypeName(String bookTypeName) {
		this.mBookTypeName = bookTypeName;
	}

}