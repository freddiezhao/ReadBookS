<?xml version="1.0" encoding="utf-8"?>
<com.sina.book.ui.widget.IgnorePressedRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:paddingTop="7dp"
    android:paddingBottom="5.33dp"
    android:baselineAligned="false"
    android:orientation="vertical">

    <include
        android:id="@+id/shelf_book1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="8.33dp"
        layout="@layout/vw_shelf_book_item" />

    <include
        android:id="@+id/shelf_book2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        layout="@layout/vw_shelf_book_item" />

    <include
        android:id="@+id/shelf_book3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="8.33dp"
        layout="@layout/vw_shelf_book_item" />

</com.sina.book.ui.widget.IgnorePressedRelativeLayout>